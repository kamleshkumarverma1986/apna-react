{
  "presets": [
    [
      "@babel/preset-react",
      {
        "pragma": "ApnaReact.createElement", // default pragma is React.createElement (only in classic runtime)
        "pragmaFrag": "ApnaReact.Fragment", // default is React.Fragment (only in classic runtime)
        "throwIfNamespace": false, // defaults to true
        "runtime": "classic" // defaults to classic
        // "importSource": "custom-jsx-library" // defaults to react (only in automatic runtime)
      }
    ]
  ]
}